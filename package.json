{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "files": [
      "test/**/*"
    ],
    "helpers": [
      "test/helpers/**/*"
    ],
    "require": [
      "@babel/register"
    ],
    "sources": [
      "src/**/*"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "roarr": "^2.13.3",
    "strip-ansi": "^5.2.0"
  },
  "description": "Intercepts stdout/ stderr.",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "ava": "^2.2.0",
    "babel-plugin-istanbul": "^5.2.0",
    "coveralls": "^3.0.5",
    "delay": "^4.3.0",
    "eslint": "^6.1.0",
    "eslint-config-canonical": "^17.3.3",
    "flow-bin": "^0.104.0",
    "flow-copy-source": "^2.0.7",
    "husky": "^3.0.2",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.19",
    "sinon": "^7.3.2"
  },
  "engines": {
    "node": ">6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "interceptor",
    "stdout",
    "stderr"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "output-interceptor",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/output-interceptor"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "generate-readme": "gitdown ./.README/README.md --output-file ./README.md",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --verbose --serial"
  },
  "version": "2.0.0"
}
