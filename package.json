{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "babel": {
      "compileAsTests": [
        "test/helpers/**/*"
      ]
    },
    "files": [
      "test/output-interceptor/**/*"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "roarr": "^2.14.6",
    "strip-ansi": "^6.0.0"
  },
  "description": "Intercepts stdout/ stderr.",
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-flow-strip-types": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "ava": "^3.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "coveralls": "^3.0.9",
    "delay": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-canonical": "^18.1.0",
    "flow-bin": "^0.117.0",
    "flow-copy-source": "^2.0.9",
    "husky": "^4.2.1",
    "nyc": "^15.0.0",
    "semantic-release": "^17.0.1",
    "sinon": "^8.1.1"
  },
  "engines": {
    "node": ">6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "interceptor",
    "stdout",
    "stderr"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "output-interceptor",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/output-interceptor"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "generate-readme": "gitdown ./.README/README.md --output-file ./README.md",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --verbose --serial"
  },
  "version": "2.0.0"
}
