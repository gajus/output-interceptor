{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "roarr": "^2.12.1",
    "strip-ansi": "^5.0.0"
  },
  "description": "Intercepts stdout/ stderr.",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-canonical": "^15.0.1",
    "flow-bin": "^0.89.0",
    "flow-copy-source": "^2.0.2",
    "husky": "^1.2.1",
    "nyc": "^13.1.0",
    "semantic-release": "^15.13.1",
    "sinon": "^7.2.2"
  },
  "engines": {
    "node": ">6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test && npm run build"
    }
  },
  "keywords": [
    "interceptor",
    "stdout",
    "stderr"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "output-interceptor",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/output-interceptor"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "generate-readme": "gitdown ./.README/README.md --output-file ./README.md",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --verbose --serial"
  },
  "version": "1.0.0"
}
